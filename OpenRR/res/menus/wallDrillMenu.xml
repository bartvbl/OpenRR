<menu>
	<container x="100% - 95px" y="100% - 200px" width="95px" height="184px" name="menuRoot">
		<image
			name="menuBackground"
			x="0px"
			y="0px"
			width="95px"
			height="184px"
			image="wallDrill_Background"
		/>
		<imageButton
			name="backButton"
			x="4px"
			y="142px"
			width="27px"
			height="28px"
			overImage="BuildMenu_Back_Over" 
			downImage="BuildMenu_Back_Down"
			onClick="switchToMain" 
		/>
		<imageButton
			name="drillWallButton"
			x="37px"
			y="135px"
			width="40px"
			height="40px"
			upImage="wallDrill_Drill_Up" 
			overImage="wallDrill_Drill_Up" 
			downImage="wallDrill_Drill_Down"
			disabledImage="wallDrill_Drill_Disabled" 
			onClick="requestDrillWall" 
		/>
		<imageButton
			name="blastWallButton"
			x="37px"
			y="95px"
			width="40px"
			height="40px"
			upImage="wallDrill_Use_Dynamite_Up" 
			overImage="wallDrill_Use_Dynamite_Up" 
			downImage="wallDrill_Use_Dynamite_Down"
			disabledImage="wallDrill_Use_Dynamite_Disabled" 
			onClick="requestBlastWall" 
		/>
		<imageButton
			name="reinforceWallButton"
			x="37px"
			y="55px"
			width="40px"
			height="40px"
			upImage="wallDrill_Reinforce_Up" 
			overImage="wallDrill_Reinforce_Up" 
			downImage="wallDrill_Reinforce_Down"
			disabledImage="wallDrill_Reinforce_Disabled" 
			onClick="requestReinforceWall" 
		/>
		<imageButton
			name="cancelDrillButton"
			x="37px"
			y="15px"
			width="40px"
			height="40px"
			upImage="wallDrill_Stop_Drill_Up" 
			overImage="wallDrill_Stop_Drill_Up" 
			downImage="wallDrill_Stop_Drill_Down"
			disabledImage="wallDrill_Stop_Drill_Disabled" 
			onClick="abortWallDrill" 
		/>
	</container>
</menu>