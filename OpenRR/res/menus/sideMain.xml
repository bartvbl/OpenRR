			downImage="smallVehicleMenu_Down"
			disabledImage="smallVehicleMenu_Disabled" 
			onClick="switchToSmallVehiclesMenu" 
		/>
		<imageButton
			name="largeVehiclesButton"
			x="6px"
			y="8px"
			width="40px"
			height="40px"
