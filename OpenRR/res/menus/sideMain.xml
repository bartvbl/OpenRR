			name="menuBackground"
			x="0px"
			y="0px"
			width="60px"
			height="184px"
			image="ingameMenu_General_MenuBackground_4"
		/>
		<imageButton
			name="teleportRaiderButton"
			x="6px"
			y="68px"
			width="40px"
			height="40px"
			upImage="teleportNewRockRaiderButton_Up" 
			overImage="teleportNewRockRaiderButton_Over" 
			downImage="teleportNewRockRaiderButton_Down"
